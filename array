import java.util.Scanner;

class array{

    public static void main(String[] args){
        Scanner scan =new Scanner(System.in);
        System.out.println("Enter a number to see its multiplication table:");
        int n = scan.nextInt();
        for(int i = 1 ; i <= 10 ; i++) {
            System.out.println(i + " x  " +  n  +  "= "  + n*i);
        }

    }
}
